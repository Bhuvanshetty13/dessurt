{
    "name": "Forms17_staggerFull_neg20_new25",
    "cuda": true,
    "gpu": 0,
    "super_computer":true,
    "save_mode": "state_dict",
    "override": true,
    "data_loader": {
        "data_set_name": "FormsBoxDetect",
        "data_dir": "../data/forms17",
        "batch_size": 2,
        "shuffle": true,
        "num_workers": 2,
        "crop_to_page":false,
        "color":false,
        "rescale_range": [0.4,0.65],
        "crop_params": {
            "crop_size":[652,1608],
            "pad":0
        },
        "no_blanks": true,
        "swap_circle":true,
        "no_graphics":true,
        "cache_resized_images": true,
        "only_types": {
            "boxes":true
        },
        "rotation": false


    },
    "validation": {
        "shuffle": false,
        "crop_to_page":false,
        "color":false,
        "rescale_range": [0.52,0.52],
        "only_types": {
            "boxes":true
        },
        "no_blanks": true,
        "swap_circle":true,
        "no_graphics":true,
        "batch_size": 1,
        "rotation": false
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.01,
        "weight_decay": 0
    },
    "loss": {
        "box":"YoloLoss"
    },
    "loss_params": {
        "box": {
            "ignore_thresh": 0.5,
            "bad_conf_weight": 20.0
        }
    },
    "loss_weights":{"box":1.0},
    "metrics": [],
    "trainer": {
        "class": "BoxDetectTrainer",
        "iterations": 250000,
        "save_dir": "saved/",
        "val_step": 10000,
        "save_step": 50000,
        "save_step_minor": 500,
        "log_step": 500,
        "verbosity": 1,
        "monitor": "loss",
        "monitor_mode": "none",
        "warmup_steps": 1000,
        "thresh_conf":0.88,
        "thresh_intersect":0.4
    },
    "arch": "YoloBoxDetector", 
    "model": {
        "color":false,
        "number_of_box_types": 2,
        "number_of_point_types": 0,
        "number_of_pixel_types": 0,
        "norm_type": "group_norm",
        "dropout": true,
        "down_layers_cfg": [1,"k5-64", "M", 
                                "hd1-128", "vd1-128", "hd2-128", 128,  "M", 
                                "hd2-256", "vd1-256", "hd4-256", 256,  "M", 
                                "hd4-512", "vd1-512", "hd8-512", 512,  "M", 
                                "hd8-1024","vd1-1024","hd16-1024",1024, 1024],
        "up_layers_cfg":[],
        "anchors_file": "anchors_noRot_new_25.json",
        "rotation":false
    }
}
