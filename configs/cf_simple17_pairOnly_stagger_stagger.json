{
    "name": "Simple17_pairOnly_stagger_stagger",
    "cuda": true,
    "gpu": 0,
    "save_mode": "state_dict",
    "override": true,
    "super_computer":true,
    "data_loader": {
        "data_set_name": "FormsBoxPair",
        "simple_dataset": true,
        "data_dir": "../data/forms17",
        "batch_size": 2,
        "shuffle": true,
        "num_workers": 2,
        "crop_to_page":false,
        "color":false,
        "rescale_range": [0.4,0.65],
        "crop_params": {
            "crop_size":[652,1608],
	    "pad":0
        },
        "no_blanks": true,
        "swap_circle":true,
        "no_graphics":true,
        "only_form_stuff":true,
        "cache_resized_images": true,
        "rotation": false,
	"use_doughnut_mask": true,
	"use_vdist_mask": true,
	"use_hdist_mask": true


    },
    "validation": {
        "simple_dataset": true,
        "shuffle": false,
        "crop_to_page":false,
        "color":false,
        "rescale_range": [0.52,0.52],
        "no_blanks": true,
        "swap_circle":true,
        "no_graphics":true,
        "only_form_stuff":true,
        "batch_size": 1,
        "rotation": false,
	"use_doughnut_mask": true,
	"use_vdist_mask": true,
	"use_hdist_mask": true
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.1,
        "weight_decay": 0
    },
    "loss": "YoloLoss",
    "loss_params": 
        {
            "ignore_thresh": 0.5,
            "bad_conf_weight": 20.0
        },
    "metrics": [],
    "trainer": {
        "class": "BoxPairTrainer",
        "iterations": 250000,
        "save_dir": "saved/",
        "val_step": 5000,
        "save_step": 50000,
        "save_step_minor": 500,
        "log_step": 500,
        "verbosity": 1,
        "monitor": "loss",
        "monitor_mode": "none",
        "warmup_steps": 1000
    },
    "arch": "PairingBoxFull", 
    "model": {
        "detector_checkpoint": "saved/Forms17_stagger_neg20_new25/checkpoint-iteration150000.pth.tar",
        "pairer_config": {
	    "num_dist_masks": 3,
            "norm_type": "group_norm",
            "up_sample_ch": 128,
            "down1_layers_cfg": ["k5-32", "M"],
            "down2_layers_cfg": [128, 128, "M",
                                    "hd2-256", "vd1-256", "hd4-256", 256,  "M",
                                    "hd4-512", "vd1-512", "hd8-512", 512,  "M"],
            "final_layers_cfg": [1024,"hd8-1024","vd1-1024","hd16-1024",1024]
        }
    }
}
