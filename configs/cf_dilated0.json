{
    "name": "AI2D_UNetDilated0",
    "cuda": true,
    "gpu": 1,
    "override": true,
    "data_loader": {
        "data_set_name": "AI2D",
        "data_dir": "/home/brianld/data/ai2d",
        "batch_size": 14,
        "shuffle": true,
        "num_workers": 2,
        "patch_size": 300,
        "center_jitter": 0.1,
        "size_jitter": 0.2
    },
    "validation": {
        "validation_split": 0.01,
        "shuffle": true
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.001,
        "weight_decay": 0
    },
    "loss": "sigmoid_BCE_loss",
    "metrics": ["meanIOU"],
    "trainer": {
        "iterations": 1000000,
        "save_dir": "saved/",
        "val_step": 2500,
        "save_step": 10000,
        "log_step": 250,
        "verbosity": 2,
        "monitor": "val_meanIOU",
        "monitor_mode": "none"
    },
    "arch": "UNetDilated", 
    "model": {
        "skip_last_sigmoid": true,
        "n_channels": 4,
        "norm_type": "weightNorm",
        "dilation_positions": []
    }
}
