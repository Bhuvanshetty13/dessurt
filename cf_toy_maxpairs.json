{
    "name": "toy_graph",
    "cuda": true,
    "gpu": 0,
    "override": true,
    "data_loader": {
        "data_set_name": "RandomMaxPairsDataset",
        "data_dir": ".",
        "batch_size": 1,
        "shuffle": false,
        "num_workers": 9,
        "max_nodes": 7
    },
    "validation": {
    },

    
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.001
    },
    "loss": "sigmoid_BCE_loss",
    "metrics": [],
    "trainer": {
        "class": "MaxPairsMetaGraphTrainer",
        "iterations": 40000,
        "save_dir": "saved/",
        "val_step": 500,
        "save_step": 5000,
        "save_step_minor": 250,
        "log_step": 250,
        "verbosity": 1,
        "monitor": "loss",
        "monitor_mode": "none",
        "adapt_lr": false,
        "use_learning_schedule": "step",
        "lr_steps": [20000,30000]
    },
    "arch": "MetaGraphNet", 
    "model": {
        "in_channels": 256,
        "node_out": 0,
        "edge_out": 1,
        "num_layers": 2,
        "repetitions": 4,
        "use_repetition_res": false,
        "num_heads": 1,
        "att_mod": "learned",
        "avg_edges": false,
        "soft_prune_edges": false,
        "prune_with_classifier": false,
        "encode_type": "fc+attention",
        "force_encoding" : true,
        "rcrhdn_size": 0,
        "relu_node_act": false,
        "infeats": 1
    }
}
