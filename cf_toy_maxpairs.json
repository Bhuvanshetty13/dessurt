{
    "name": "toy_graph",
    "cuda": true,
    "gpu": 0,
    "override": true,
    "data_loader": {
        "data_set_name": "RandomMaxPairsDataset",
        "data_dir": ".",
        "batch_size": 1,
        "shuffle": false,
        "num_workers": 9,
        "max_nodes": 7
    },
    "validation": {
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.001
    },
    "loss": "sigmoid_BCE_loss",
    "metrics": [],
    "trainer": {
        "class": "MaxPairsMetaGraphTrainer",
        "iterations": 20000,
        "save_dir": "saved/",
        "val_step": 500,
        "save_step": 5000,
        "save_step_minor": 250,
        "log_step": 250,
        "verbosity": 1,
        "monitor": "loss",
        "monitor_mode": "none",
        "adapt_lr": false
    },
    "arch": "MetaGraphNet", 
    "model": {
        "in_channels": 256,
        "node_out": 0,
        "edge_out": 1,
        "num_layers": 2,
        "repetitions": 4,
        "use_reptition_res": true,
        "num_heads": 4,
        "soft_prune_edges": false,
        "prune_with_classifier": false,
        "encode_type": "fc+attention",
        "force_encoding" : true,
        "infeats": 1
    }
}
