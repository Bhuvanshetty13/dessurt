{
    "name": "forms10_iouVGG_fast",
    "cuda": true,
    "gpu": 0,
    "save_mode": "model",
    "override": true,
    "data_loader": {
        "data_set_name": "FormsBoxDetect",
        "data_dir": "/home/ubuntu/brian/data/forms",
        "batch_size": 2,
        "shuffle": true,
        "num_workers": 2,
        "crop_to_page":false,
        "color":false,
        "rescale_range": [0.4,0.65],
        "crop_params": {
            "crop_size":1024
        },
        "no_blanks": true,
        "swap_circle":true,
        "no_graphics":true,
        "cache_resized_images": true,
        "only_types": {
            "boxes":true
        },
        "rotation": false


    },
    "validation": {
        "shuffle": false,
        "crop_to_page":false,
        "color":false,
        "rescale_range": [0.52,0.52],
        "only_types": {
            "boxes":true
        },
        "no_blanks": true,
        "swap_circle":true,
        "no_graphics":true,
        "batch_size": 1,
        "rotation": false
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.01,
        "weight_decay": 0
    },
    "loss": {
        "box":"YoloLoss"
    },
    "loss_params": {
        "box": {
            "ignore_thresh": 0.5
        }
    },
    "loss_weights":{"box":1.0},
    "metrics": [],
    "trainer": {
        "class": "BoxDetectTrainer",
        "iterations": 100000,
        "save_dir": "saved/",
        "val_step": 2000,
        "save_step": 4000,
        "log_step": 250,
        "verbosity": 1,
        "monitor": "loss",
        "monitor_mode": "none",
        "warmup_steps": 3000
    },
    "arch": "YoloBoxDetector", 
    "model": {
        "color":false,
        "number_of_box_types": 2,
        "number_of_point_types": 0,
        "number_of_pixel_types": 0,
        "norm_type": "group_norm",
        "Xdown_layers_cfg": [1,"C64", "M", "R64", "R64", "D128", "R128", "R128", "D256", "R256", "R256", "D512", "R512", "R512", "D1024", "R1024", "R1024", "U1024", "R1024", "ReLU"],
        "down_layers_cfg": [1,64, "M", 128, 128, "M", 256, 256, "M", 512, 512, "M", 1024, 1024, 1024],
        "up_layers_cfg":[],
        "anchors_file": "anchors_noRot_12.json",
        "rotation":false
    }
}
