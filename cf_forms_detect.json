{
    "name": "Forms_detect",
    "cuda": true,
    "gpu": 0,
    "override": true,
    "data_loader": {
        "data_set_name": "FormsDetect",
        "data_dir": "/home/ubuntu/brian/data/forms",
        "batch_size": 16,
        "shuffle": true,
        "num_workers": 3,
        "crop_to_page":false,
        "crop_params": {
            "prob_label": 0.75
            "crop_size":256
        },
        "rescale_range": [384, 640]

    },
    "validation": {
        "shuffle": false,
        "crop_to_page":false,
        "rescale_range": [512,512]
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.001,
        "weight_decay": 0
    },
    "loss": "detect_alignment_loss",
    "metrics": [],
    "trainer": {
        "iterations": 1000000,
        "save_dir": "saved/",
        "val_step": 5000,
        "save_step": 10000,
        "log_step": 500,
        "verbosity": 2,
        "monitor": "loss",
        "monitor_mode": "none"
    },
    "arch": "SOL_EOL_Finder", 
    "model": {
        "number_of_object_types": 4,
        "norm_type": "weightNorm"
    }
}
