{
    "name": "Forms18_newRes",
    "cuda": true,
    "gpu": 0,
    "super_computer":true,
    "save_mode": "state_dict",
    "override": true,
    "data_loader": {
        "data_set_name": "FormsBoxDetect",
        "data_dir": "../data/forms",
        "batch_size": 2,
        "shuffle": true,
        "num_workers": 2,
        "crop_to_page":false,
        "color":false,
        "rescale_range": [0.4,0.65],
        "crop_params": {
            "crop_size":[652,1608],
            "pad":0
        },
        "no_blanks": true,
        "swap_circle":true,
        "no_graphics":true,
        "cache_resized_images": true,
        "only_types": {
            "boxes":true
        },
        "rotation": false


    },
    "validation": {
        "shuffle": false,
        "crop_to_page":false,
        "color":false,
        "rescale_range": [0.52,0.52],
        "only_types": {
            "boxes":true
        },
        "no_blanks": true,
        "swap_circle":true,
        "no_graphics":true,
        "batch_size": 1,
        "rotation": false
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.01,
        "weight_decay": 0
    },
    "loss": {
        "box":"YoloLoss"
    },
    "loss_params": {
        "box": {
            "ignore_thresh": 0.5,
            "bad_conf_weight": 20.0
        }
    },
    "loss_weights":{"box":1.0},
    "metrics": [],
    "trainer": {
        "class": "BoxDetectTrainer",
        "iterations": 200000,
        "save_dir": "saved/",
        "val_step": 10000,
        "save_step": 50000,
        "save_step_minor": 500,
        "log_step": 500,
        "verbosity": 1,
        "monitor": "loss",
        "monitor_mode": "none",
        "warmup_steps": 1000,
        "thresh_conf":0.88,
        "thresh_intersect":0.4
    },
    "arch": "YoloBoxDetector", 
    "model": {
        "color":false,
        "number_of_box_types": 2,
        "number_of_point_types": 0,
        "number_of_pixel_types": 0,
        "norm_type": "group_norm",
        "dropout":true,
        "down_layers_cfg": [1,"k7-32", "M", "R64", "R64", "wave1,2-64",  "D128",
                                "wave1,2-128", "R128", "wave1,4-128", "D256",
                                "wave1,4-256", "R256", "wave1,8-256", "D512",
                                "wave1,8-512", "R512", "wave1,16-512", "R512", "R512"],
        "up_layers_cfg":[],
        "anchors_file": "anchors_noRot_new_25.json",
        "rotation":false
    }
}
