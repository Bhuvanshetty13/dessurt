{
    "name": "toy_graph",
    "cuda": true,
    "gpu": 0,
    "override": true,
    "data_loader": {
        "data_set_name": "RandomMessagesDataset",
        "data_dir": ".",
        "batch_size": 1,
        "shuffle": false,
        "num_workers": 0,
        "max_pairs": 1,
        "max_nodes": 4
    },
    "validation": {
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.001
    },
    "loss": "sigmoid_BCE_loss",
    "metrics": [],
    "trainer": {
        "class": "ToyMetaGraphTrainer",
        "iterations": 50000,
        "save_dir": "saved/",
        "val_step": 500,
        "save_step": 5000,
        "save_step_minor": 250,
        "log_step": 250,
        "verbosity": 1,
        "monitor": "loss",
        "monitor_mode": "none",
        "adapt_lr": false
    },
    "arch": "MetaGraphNet", 
    "model": {
        "in_channels": 256,
        "layer_type": "attention",
        "node_out": 10,
        "edge_out": 0,
        "num_layers": 2,
        "repitions":3,
        "num_heads": 4,
        "encode_type": "fc",
        "infeats": 12,
        "dropout": null
    }
}
