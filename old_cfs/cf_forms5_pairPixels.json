{
    "name": "Forms5_pairPixels",
    "cuda": true,
    "gpu": 0,
    "override": true,
    "data_loader": {
        "data_set_name": "FormsPair",
        "data_dir": "/home/ubuntu/brian/data/forms",
        "batch_size": 28,
        "patch_size": 300,
        "shuffle": true,
        "num_workers": 6,
        "patch_size": 300,
        "center_jitter": 0.1,
        "size_jitter": 0.2
    },
    "validation": {
        "shuffle": true,
        "patch_size": 300,
        "center_jitter": 0,
        "size_jitter": 0
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.001,
        "weight_decay": 0
    },
    "loss": "sigmoid_BCE_loss",
    "metrics": ["meanIOU"],
    "trainer": {
        "iterations": 1000000,
        "save_dir": "saved/",
        "val_step": 5000,
        "save_step": 5000,
        "log_step": 50,
        "verbosity": 2,
        "monitor": "val_meanIOU",
        "monitor_mode": "none"
    },
    "arch": "UNet", 
    "model": {
        "skip_last_sigmoid": true,
        "n_channels": 4,
        "norm_type": "weightNorm"
    }
}
